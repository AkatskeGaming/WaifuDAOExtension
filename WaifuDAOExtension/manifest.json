{
  "name": "WaifuDAOExtension",
  "description": "This extension is mainly aimed at providing an automatic Rarity calculator for Digital Eyes Marketplace.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [{
    "matches": ["*://*.digitaleyes.market/collections/Waifu%20DAO%20Gaming%20Guild*"],
    "js": [
      "rarityCalculator.js",
      "digitalEyesRarity.js"
    ],
    "css": ["rarity.css"],
    "run_at": "document_end"
  }],
  "action": {
    "default_popup": "waifu.html",
    "default_icon": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    },
    "default_title": "WaifuDaoExtension"
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  },
  "options_page": "waifu.html"
}
